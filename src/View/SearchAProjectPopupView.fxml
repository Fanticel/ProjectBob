<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.SearchAProjectPopupViewController">
   <HBox alignment="CENTER" prefHeight="78.0" prefWidth="600.0">
      <children>
         <Text fill="#f5ba41" strokeType="OUTSIDE" strokeWidth="0.0" text="Search">
            <font>
               <Font name="System Bold" size="25.0" />
            </font>
         </Text>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Text fill="#f5ba41" strokeType="OUTSIDE" strokeWidth="0.0" text="NAME: ">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Text>
         <TextField fx:id="nameField" style="-fx-background-color: white; -fx-border-color: #f5ba41; -fx-border-radius: 10; -fx-border-width: 2;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </TextField>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Text fill="#f5ba41" strokeType="OUTSIDE" strokeWidth="0.0" text="TYPE:">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Text>
         <ChoiceBox fx:id="typeChoiceBox" prefWidth="150.0" style="-fx-background-color: #f5ba41;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </ChoiceBox>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Text fill="#f5ba41" strokeType="OUTSIDE" strokeWidth="0.0" text="PRICE RANGE:">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Text>
         <TextField fx:id="priceRangeMin" promptText="Min" style="-fx-background-color: white; -fx-border-color: #f5ba41; -fx-border-radius: 10; -fx-border-width: 2;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </TextField>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="-">
            <font>
               <Font size="20.0" />
            </font>
            <HBox.margin>
               <Insets top="10.0" />
            </HBox.margin>
         </Text>
         <TextField fx:id="priceRangeMax" promptText="Max" style="-fx-background-color: white; -fx-border-color: #f5ba41; -fx-border-radius: 10; -fx-border-width: 2;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </TextField>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Text fill="#f5ba41" strokeType="OUTSIDE" strokeWidth="0.0" text="MAN HOURS:">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Text>
         <TextField fx:id="manHoursMin" promptText="Min" style="-fx-background-color: white; -fx-border-color: #f5ba41; -fx-border-radius: 10; -fx-border-width: 2;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </TextField>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="-">
            <font>
               <Font size="20.0" />
            </font>
            <HBox.margin>
               <Insets top="10.0" />
            </HBox.margin>
         </Text>
         <TextField fx:id="manHoursMax" promptText="Max" style="-fx-background-color: white; -fx-border-color: #f5ba41; -fx-border-radius: 10; -fx-border-width: 2;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </TextField>
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Text fill="#f5ba41" strokeType="OUTSIDE" strokeWidth="0.0" text="STATUS: ">
            <font>
               <Font name="System Bold" size="20.0" />
            </font>
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Text>
         <ChoiceBox fx:id="statusChoiceBox" prefWidth="150.0" style="-fx-background-color: #f5ba41;">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </ChoiceBox>
      </children>
   </HBox>
   <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
         <Button fx:id="searchButton" mnemonicParsing="false" onAction="#clickSearchButt" style="-fx-background-color: #f5ba41;" text="Search">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="50.0" top="10.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="backButton" mnemonicParsing="false" onAction="#clickBackButt" style="-fx-background-color: #f5ba41;" text="Back">
            <HBox.margin>
               <Insets bottom="10.0" left="50.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Button>
      </children>
   </HBox>
</VBox>
