    <children>
        <fx:include fx:id="createProject" source="CreateProjectView.fxml" />
        <StackPane fx:id="stackPane" prefHeight="400.0" prefWidth="529.0">
            <children>
                <fx:include fx:id="residentialProjectView" source="EmbeddedResidentialProjectView.fxml" visible="true" />
                <fx:include fx:id="commercialProjectView" source="EmbeddedCommercialProjectView.fxml" visible="false" />
                <fx:include fx:id="industrialProjectView" source="EmbeddedIndustrialProjectView.fxml" visible="false" />
                <fx:include fx:id="roadProjectView" source="EmbeddedRoadProjectView.fxml" visible="false" />
